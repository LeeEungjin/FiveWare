<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="noticeFileMapper">
  	<select id="selectList" parameterType="int" resultType="com.five.ware.erp.notice.NoticeFileDTO">
  		select * from noticefile where num=#{num}
  	</select>
  	
  	<insert id="insert" parameterType="com.five.ware.erp.notice.NoticeFileDTO">
  		insert into noticefile values(notice_seq.nextval, #{num}, #{fileName}, #{oriName})
  	</insert>
  	
  	<delete id="delete" parameterType="int">
  		delete from noticefile where fnum=#{fnum}
  	</delete>  
  	
  </mapper>